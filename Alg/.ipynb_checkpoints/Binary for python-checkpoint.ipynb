   "source": [
    "#Binary numbers\n",
    "n = int(input().strip())\n",
    "cnt=0\n",
    "p_cnt=0\n",
    "n<<=1\n",
    "while n:\n",
    "    n >>= 1\n",
    "    if n&1:\n",
    "        cnt+=1\n",
    "    else:\n",
    "        if cnt > p_cnt:\n",
    "            p_cnt = cnt\n",
    "        cnt = 0\n",
    "print(p_cnt)"
   ]
