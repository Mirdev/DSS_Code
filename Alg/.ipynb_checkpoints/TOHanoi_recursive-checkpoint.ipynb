    "#hanoi\n",
    "\n",
    "def hanoi(n, F, A, T):\n",
    "    if n==1:\n",
    "        print(\"%d, %s->%s\"%(n,F,T))\n",
    "    else:\n",
    "        hanoi(n-1, F, T, A)\n",
    "        hanoi(1, F, A, T)\n",
    "        hanoi(n-1, A, F, T)\n",
    "    \n",
    "hanoi(3, \"a\", \"b\", \"c\")"
   ]
  }
